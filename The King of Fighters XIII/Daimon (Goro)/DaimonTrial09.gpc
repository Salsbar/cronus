	set_val(XB360_X, 100);
	wait(100);
	set_val(XB360_X, 0);
	
//Combo Wait
	wait(150);
	
//Standing LP
	set_val(XB360_X, 100);
	wait(100);
	set_val(XB360_X, 0);
	
//Combo Wait
	wait(150);
	
//Standing LK
	set_val(XB360_A, 100);
	wait(40);
	set_val(XB360_A, 0);
	
//Combo Wait
	wait(150);
	
