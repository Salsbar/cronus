//Trail 09 for Daimon (Goro) in Kof13. Stand next to target and press LS to start the combo (Push computer to the edge of the right screen).


//Start Combo
main {  if(get_val(XB360_LS)) combo_run(Daimon09); 
        if(get_val(XB360_RS)) combo_stop(Daimon09);
}

//Main Script
combo Daimon09 {
	
//High Jumping HK
	set_val(XB360_UP, 100);
	set_val(XB360_RIGHT, 100);
	wait(200);
	set_val(XB360_UP, 0);
	set_val(XB360_RIGHT, 0);
	wait(250);
	set_val(XB360_B, 100);
	wait(160);
	set_val(XB360_B, 0);
	
//Combo Wait
	wait(200);
	
